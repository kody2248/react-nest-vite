{
	"name": "react-nest-vite",
	"version": "1.0.0",
	"description": "A tool used to build and maintain Diablo 2 trade lists for pasting into Discord Servers.",
	"author": "Kody Sager",
	"license": "MIT",
	"scripts": {
		"start:dev": "concurrently \"npm run start:dev --workspace=server\" \"npm run start:dev --workspace=client\"",
		"start:prod": "npm run start:prod --workspaces"
	},
	"private": "true",
	"workspaces": [
		"packages/*"
	],
	"devDependencies": {
		"concurrently": "^7.5.0"
	}
}
